{
  "version": 2,
  "builds": [
    {
      "src": "web_api.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web_api.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
